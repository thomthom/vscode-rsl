{
  "name": "rsl",
  "displayName": "RSL",
  "description": "Syntax highlighting for RSL (Robot Scripting Language)",
  "version": "0.0.1",
  "publisher": "evil-software-empire",
  "repository": {
    "type": "git",
    "url": "https://github.com/thomthom/vscode-rsl"
  },
  "icon": "rsl-logo-128x128.png",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "rsl",
        "aliases": [
          "Robot Scripting Language",
          "rsl"
        ],
        "extensions": [
          ".rsl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rsl",
        "scopeName": "source.rsl",
        "path": "./syntaxes/rsl.tmLanguage.json"
      },
      {
        "scopeName": "markdown.rsl.codeblock",
        "path": "./syntaxes/rsl-markdown-injection.tmLanguage.json",
        "injectTo": [
          "text.html.markdown"
        ]
      }
    ]
  }
}
